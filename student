package student;
import java.util.ArrayList;
/**
 *
 * @author USER
 */
public class studentclass {
      private String name;
    private int id;

    
    public studentclass(String name, int id) {
        this.name = name;
        this.id = id;
    }

    
    public void displayinfo() {
        System.out.println("Student Name: " + name + ", ID: " + id);
    }
    
  
    public String getname() {
        return name;
    }

    public int getid() {
        return id;
    }
}
